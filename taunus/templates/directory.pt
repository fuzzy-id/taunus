<html lang="en" 
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="base">
  <tal:block metal:fill-slot="body">
    <div class="data spec">
      <table>
	<tr> 
	  <th>File</th>
	</tr>
	<tr tal:repeat="item listing">
	  <td><a href="${item.path}">${item}</a></td>
	</tr>
      </table>
    </div> <!-- closes data -->
  </tal:block>
</html>
